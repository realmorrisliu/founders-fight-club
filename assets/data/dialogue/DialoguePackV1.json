      },
      "post_fight": {
        "en": ["Loser: Need fewer parameters.", "Winner: Need more GPUs."],
        "zh": ["败者：参数太多了。", "胜者：GPU 还不够。"]
      }
    },
    {
      "id": "zef_vs_jensen",
      "fighters": ["zef_bezos", "jensen_hwang"],
      "pre_fight": {
        "en": ["Zef: I deliver scale.", "Jensen: I deliver speed."],
        "zh": ["Zef：我交付规模。", "Jensen：我交付速度。"]
      },
      "post_fight": {
        "en": ["Loser: Shipment delayed.", "Winner: Shipment accelerated."],
        "zh": ["败者：发货延迟。", "胜者：发货加速。"]
      }
    },
    {
      "id": "bill_vs_larry",
      "fighters": ["bill_geytz", "larry_pagyr"],
      "pre_fight": {
        "en": ["Bill: Legacy still runs the world.", "Larry: Search says no."],
