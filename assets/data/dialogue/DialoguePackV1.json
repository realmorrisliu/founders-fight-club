{
  "version": "v1",
  "tone": "comedy_satire",
  "fighters": {
    "elon_mvsk": {
      "name": "Elon Mvsk",
      "intro": {
        "en": [
          "Cage first, thread second.",
          "Post less. Punch more."
        ],
        "zh": [
          "先进笼，再发帖。",
          "少发帖，多出拳。"
        ]
      },
      "win": {
        "en": [
          "Algorithm says I was inevitable.",
          "Launch successful. Again."
        ],
        "zh": [
          "算法说我注定会赢。",
          "发射成功。又一次。"
        ]
      }
    },
    "mark_zuck": {
      "name": "Mark Zuck",
      "intro": {
        "en": [
          "You challenge. I schedule.",
          "Welcome to the octagon roadmap."
        ],
        "zh": [
          "你负责挑战，我负责排期。",
          "欢迎来到八角笼路线图。"
        ]
      },
      "win": {
        "en": [
          "Engagement completed.",
          "You were A/B tested."
        ],
        "zh": [
          "互动闭环已完成。",
          "你刚被 A/B 测试了。"
        ]
      }
    },
    "sam_altmyn": {
      "name": "Sam Altmyn",
      "intro": {
        "en": [
          "Let's benchmark your confidence.",
          "Prompt carefully."
        ],
        "zh": [
          "来，基准测试一下你的自信。",
          "先把提示词想清楚。"
        ]
      },
      "win": {
        "en": [
          "Iteration beats ego.",
          "Model updated. You are deprecated."
        ],
        "zh": [
          "迭代，胜过自负。",
          "模型已更新，你已过时。"
        ]
      }
    },
    "peter_thyell": {
      "name": "Peter Thyell",
      "intro": {
        "en": [
          "Control is a feature, not a bug.",
          "Sign here, then fall here."
        ],
        "zh": [
          "控制不是漏洞，是功能。",
          "先在这里签字，再在这里倒下。"
        ]
      },
      "win": {
        "en": [
          "Term accepted.",
          "Liquidity always wins."
        ],
        "zh": [
          "条款已接受。",
          "流动性永远获胜。"
        ]
      }
    },
    "zef_bezos": {
      "name": "Zef Bezos",
      "intro": {
        "en": [
          "Your dream ships next year. Mine ships now.",
          "Prime delivery, same-day pain."
        ],
        "zh": [
          "你的梦想明年发货，我的今天送达。",
          "Prime 当日达，痛感也当日达。"
        ]
      },
      "win": {
        "en": [
          "Delivered.",
          "Your return window has closed."
        ],
        "zh": [
          "已送达。",
          "你的退货窗口已关闭。"
        ]
      }
    },
    "bill_geytz": {
      "name": "Bill Geytz",
      "intro": {
        "en": [
          "Compatibility mode: on.",
          "Please wait... forever."
        ],
        "zh": [
          "兼容模式：已开启。",
          "请稍候……一直稍候。"
        ]
      },
      "win": {
        "en": [
          "Works as designed.",
          "Did you try restarting yourself?"
        ],
        "zh": [
          "这就是设计使然。",
          "你试过重启你自己吗？"
        ]
      }
    },
    "larry_pagyr": {
      "name": "Larry Pagyr",
      "intro": {
        "en": [
          "I do not chase. I route.",
          "You are page two at best."
        ],
        "zh": [
          "我不追，我只做路径规划。",
          "你最多排在第二页。"
        ]
      },
      "win": {
        "en": [
          "Top result secured.",
          "Query resolved."
        ],
        "zh": [
          "第一结果已锁定。",
          "查询已解决。"
        ]
      }
    },
    "sergey_brinn": {
      "name": "Sergey Brinn",
      "intro": {
        "en": [
          "Let's test in production.",
          "If it breaks, it is a feature demo."
        ],
        "zh": [
          "我们直接在生产环境测试。",
          "真坏了也算功能演示。"
        ]
      },
      "win": {
        "en": [
          "Prototype validated.",
          "Moonshot approved."
        ],
        "zh": [
          "原型已验证。",
          "登月项目批准。"
        ]
      }
    },
    "sundar_pichoy": {
      "name": "Sundar Pichoy",
      "intro": {
        "en": [
          "Let's keep this efficient.",
          "Clean inputs. Clean outcomes."
        ],
        "zh": [
          "让我们高效一点。",
          "输入干净，结果也干净。"
        ]
      },
      "win": {
        "en": [
          "Shipped globally.",
          "Default setting: me winning."
        ],
        "zh": [
          "已全球发布。",
          "默认设置：我赢。"
        ]
      }
    },
    "jensen_hwang": {
      "name": "Jensen Hwang",
      "intro": {
        "en": [
          "Need more compute?",
          "You brought code. I brought cores."
        ],
        "zh": [
          "需要更多算力吗？",
          "你带了代码，我带了核心。"
        ]
      },
      "win": {
        "en": [
          "Frame rate stable. Opponent unstable.",
          "Overclock complete."
        ],
        "zh": [
          "帧率稳定，对手不稳定。",
          "超频完成。"
        ]
      }
    },
    "satya_nadello": {
      "name": "Satya Nadello",
      "intro": {
        "en": [
          "Let's collaborate... aggressively.",
          "Cloud first, fists second."
        ],
        "zh": [
          "我们来协作……激进地协作。",
          "先上云，再上拳。"
        ]
      },
      "win": {
        "en": [
          "Migration complete.",
          "Your workflow is now managed."
        ],
        "zh": [
          "迁移完成。",
          "你的工作流已被接管。"
        ]
      }
    },
    "tim_cuke": {
      "name": "Tim Cuke",
      "intro": {
        "en": [
          "Designed to dominate.",
          "Minimal form. Maximum pain."
        ],
        "zh": [
          "为统治而设计。",
          "外形极简，痛感拉满。"
        ]
      },
      "win": {
        "en": [
          "Polished. Predictable.",
          "You are out of ecosystem."
        ],
        "zh": [
          "精致，也可预测。",
          "你已被移出生态。"
        ]
      }
    },
    "jack_dorsee": {
      "name": "Jack Dorsee",
      "intro": {
        "en": [
          "Signal over noise.",
          "I brought old school chaos."
        ],
        "zh": [
          "信号高于噪音。",
          "我带来了老派混沌。"
        ]
      },
      "win": {
        "en": [
          "Thread closed.",
          "Decentralized knockout."
        ],
        "zh": [
          "话题已关闭。",
          "去中心化 KO。"
        ]
      }
    },
    "travis_kalanik": {
      "name": "Travis Kalanik",
      "intro": {
        "en": [
          "I am taking this market.",
          "Surge pricing starts now."
        ],
        "zh": [
          "这个市场我接管了。",
          "动态加价，现在开始。"
        ]
      },
      "win": {
        "en": [
          "Demand was high.",
          "Ride ended."
        ],
        "zh": [
          "需求很高。",
          "行程结束。"
        ]
      }
    },
    "reed_hestings": {
      "name": "Reed Hestings",
      "intro": {
        "en": [
          "No breaks. Next round now.",
          "Skip intro. Catch hands."
        ],
        "zh": [
          "不休息，下一回合现在开始。",
          "跳过片头，直接挨打。"
        ]
      },
      "win": {
        "en": [
          "You are still watching.",
          "Autoplay continued."
        ],
        "zh": [
          "你还在观看。",
          "自动播放继续。"
        ]
      }
    },
    "steve_jobz": {
      "name": "Steve Jobz",
      "intro": {
        "en": [
          "You shipped fast. Did you ship right?",
          "One more thing: survive."
        ],
        "zh": [
          "你发得很快，但你发对了吗？",
          "还有一件事：活下来。"
        ]
      },
      "win": {
        "en": [
          "Taste is a mechanic.",
          "Simplicity defeated you."
        ],
        "zh": [
          "审美，本身就是机制。",
          "极简，击败了你。"
        ]
      }
    }
  },
