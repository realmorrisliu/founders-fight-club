      },
      "post_fight": {
        "en": ["Loser: Outdated claim.", "Winner: Updated index."],
        "zh": ["败者：结论过时。", "胜者：索引已更新。"]
      }
    },
    {
      "id": "tim_vs_steve",
      "fighters": ["tim_cuke", "steve_jobz"],
      "pre_fight": {
        "en": ["Tim: We kept it polished.", "Steve: Did you keep it hungry?"],
        "zh": ["Tim：我们保持了精致。", "Steve：你保持了饥饿吗？"]
      },
      "post_fight": {
        "en": ["Loser: Design review failed.", "Winner: Ship taste, not noise."],
        "zh": ["败者：设计评审未通过。", "胜者：交付审美，不是噪音。"]
      }
    },
    {
      "id": "elon_vs_sam",
      "fighters": ["elon_mvsk", "sam_altmyn"],
      "pre_fight": {
        "en": ["Elon: Open speed wins.", "Sam: Safe speed wins."],
        "zh": ["Elon：开放速度会赢。", "Sam：安全速度会赢。"]
      },
      "post_fight": {
        "en": ["Loser: See you next funding round.", "Winner: Round closed."],
        "zh": ["败者：下轮融资见。", "胜者：本轮已关闭。"]
      }
    },
    {
      "id": "jack_vs_elon",
      "fighters": ["jack_dorsee", "elon_mvsk"],
      "pre_fight": {
        "en": ["Jack: I built the storm.", "Elon: I own the storm."],
        "zh": ["Jack：风暴是我造的。", "Elon：风暴现在归我。"]
      },
      "post_fight": {
        "en": ["Loser: Feed archived.", "Winner: Feed amplified."],
        "zh": ["败者：信息流已归档。", "胜者：信息流已放大。"]
      }
